<p id="notice"><%= notice %></p>

<h1>Evaluaciones</h1>

<table>
  <thead>
    <tr>
      <th>Turno</th>
      <th>Pregunta1</th>
      <th>Resultado1</th>
      <th>Pregunta2</th>
      <th>Resultado2</th>
      <th>Pregunta3</th>
      <th>Resultado3</th>
      <th>Pregunta4</th>
      <th>Resultado4</th>
      <th>Resultado total</th>
      <th>Comentario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluaciones.each do |evaluacion| %>
      <tr>
        <td><%= evaluacion.turno %></td>
        <td><%= evaluacion.pregunta1 %></td>
        <td><%= evaluacion.resultado1 %></td>
        <td><%= evaluacion.pregunta2 %></td>
        <td><%= evaluacion.resultado2 %></td>
        <td><%= evaluacion.pregunta3 %></td>
        <td><%= evaluacion.resultado3 %></td>
        <td><%= evaluacion.pregunta4 %></td>
        <td><%= evaluacion.resultado4 %></td>
        <td><%= evaluacion.resultado_total %></td>
        <td><%= evaluacion.comentario %></td>
        <td><%= link_to 'Mostrar', evaluacion %></td>
        <td><%= link_to 'Editar', edit_evaluacion_path(evaluacion) %></td>
        <td><%= link_to 'Elminar', evaluacion, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluacion', new_evaluacion_path %>
